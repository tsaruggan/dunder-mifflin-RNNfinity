{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"  
        }
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "backend/server.js"
      },
      {
        "src": "/(.*)", 
        "dest": "frontend/build/index.html"
      }
    ]
  }
  